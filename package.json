{
  "name": "mynextapp",
  "version": "0.1.0",
  "private": true,
  "main": "src/firebaseApp.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "deploy": "firebase deploy --only functions,hosting",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "format": "prettier --write --ignore-path .gitignore \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "test-all": "run-s lint format lint:fix",
    "sort": "sort-package-json package.json"
  },
  "dependencies": {
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.20.0",
    "next": "12.1.3",
    "react": "18.0.0",
    "react-dom": "18.0.0"
  },
  "devDependencies": {
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "autoprefixer": "^10.4.4",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.3",
    "eslint-config-prettier": "8.5.0",
    "postcss": "^8.4.12",
    "prettier": "2.6.1",
    "sass": "^1.49.11",
    "sort-package-json": "^1.55.0",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.3",
    "yarn-run-all": "^3.1.1"
  }
}
